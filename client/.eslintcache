console.log(res.data)\r\n        }).catch(e => {\r\n            if (axios.isCancel(e)) return \r\n        })\r\n    }, [page])\r\n\r\n    useEffect(() => {\r\n        getData()\r\n    }, [getData])\r\n\r\n\r\n    const comparePriceSearch = (value) => {\r\n        if (value !== '') {\r\n            if (typeof(Storage) !== \"undefined\") {\r\n                sessionStorage.removeItem('searchResult');\r\n                sessionStorage.setItem(\"searchResult\", value);\r\n\r\n                console.log(value)\r\n                // Complete search  \r\n                history.push({\r\n                    pathname: '/search',\r\n                    search: `?q=${sessionStorage.getItem(\"searchResult\")}`\r\n                })\r\n            } else {\r\n                console.log('No session storage support')\r\n\r\n                // Complete search with context\r\n                setSearch(value)\r\n                history.push({\r\n                    pathname: '/search',\r\n                    search: `?q=${setSearch}`\r\n                })\r\n            }\r\n        } else {\r\n            return\r\n        }\r\n    };\r\n\r\n    return (\r\n        <>\r\n        {/* Section 1 */}\r\n        <SearchBar />\r\n\r\n        {/* Section 2 */}\r\n        <Title level={3} style={{textAlign: 'center', marginBottom: '4%'}}>View todays top discounts</Title>\r\n        <Row style={{textAlign: 'center', margin: '0 3%'}}>\r\n            {/* <Col xs={0} sm={0} md={0} lg={0} xl={2} xxl={4} style={{borderRight: '2px solid #f0f0f0'}}> \r\n               <SiderNav /> \r\n            </Col> */}\r\n            <Col justify=\"center\" align=\"center\">\r\n            {products ?\r\n                <InfiniteScroll  \r\n                    dataLength={products.length}\r\n                    next={(() => setPage(page + 1))} // set up server side pagination\r\n                    hasMore={hasMore}                // replace this with get data and update page count\r\n                    //